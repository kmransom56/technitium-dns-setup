{
  "version": 64,
  "dnsServerDomain": "dns.netintegrate.net",
  "dnsServerLocalEndPoints": [
    "0.0.0.0:53",
    "[::]:53"
  ],
  "webServiceLocalAddresses": [
    "0.0.0.0"
  ],
  "webServiceHttpPort": 5380,
  "webServiceEnableHttps": true,
  "webServiceHttpsPort": 5443,
  "webServiceUseSelfSignedTlsCertificate": false,
  "webServiceTlsCertificatePath": "/etc/dns/certificates/dns.netintegrate.net_full_chain.pfx",
  "webServiceTlsCertificatePassword": "secure-cert-password-2024",
  "enableDnsOverHttp": false,
  "enableDnsOverTls": true,
  "enableDnsOverHttps": true,
  "enableDnsOverQuic": true,
  "dnsOverTlsCertificatePath": "/etc/dns/certificates/dns.netintegrate.net_full_chain.pfx",
  "dnsOverTlsCertificatePassword": "secure-cert-password-2024",
  "dnsOverHttpsCertificatePath": "/etc/dns/certificates/dns.netintegrate.net_full_chain.pfx",
  "dnsOverHttpsCertificatePassword": "secure-cert-password-2024",
  "dnsOverQuicCertificatePath": "/etc/dns/certificates/dns.netintegrate.net_full_chain.pfx",
  "dnsOverQuicCertificatePassword": "secure-cert-password-2024",
  "tsigKeys": [
    {
      "keyName": "home.netintegrate.net.",
      "sharedSecret": "base64-encoded-shared-secret",
      "algorithmName": "HMAC-SHA256"
    }
  ],
  "defaultRecordTtl": 3600,
  "useSoaSerialDateScheme": false,
  "zoneTransferAllowedNetworks": [],
  "notifyAllowedNetworks": [],
