  "tcpSendTimeout": 10000,
  "tcpReceiveTimeout": 10000,
  "quicIdleTimeout": 60000,
  "quicMaxInboundStreams": 100,
  "listenBacklog": 100,
  "webServiceUseSelfSignedTlsCertificate": false
