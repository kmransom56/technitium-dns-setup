  "allowListNextUpdatedOn": "2024-01-01T02:00:00Z",
  "enableLogging": true,
  "ignoreResolverLogs": false,
  "logQueries": true,
  "useLocalTime": false,
  "logFolder": "/etc/dns/logs",
  "maxLogFileDays": 30,
  "maxLogFileSize": 104857600,
  "preferIPv6": false,
  "udpPayloadSize": 1232,
  "dnssecValidation": true,
  "eDnsClientSubnet": false,
